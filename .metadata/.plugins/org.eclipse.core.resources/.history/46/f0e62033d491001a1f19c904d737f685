package dptest0509;

public class titling {
	
	static int[] arr = new int[1001]; 
	
	public static void main(String[] args) {
		titling t = new titling();
		int n = 5;
				
		t.dp(n);
	}
	
	public int dp(int n) {
		
		if(n == 1) return 1;
		if(n == 2) return 1;
		if(arr[n] != 0) return arr[n];
		
		
		return arr[n] = (arr[n-1] + arr[n - 2]) % 10007;
	}
}
