package vo;

public class SortTest {

	public static void main(String[] args) {
		
		int[] arr = {3,6,7,9,5};
		
		//1회전을 하면 가장 작은 값이 첫번째에 위치하게 됨
		
		for(int i=0;i<arr.length-1; i++) {
			
			int index = i;
			
			for(int j=i+1;j<arr.length; j++) {
				if(arr[j]<arr[index]) {
					index = j;
				}
			}
			
			if(i != index) {
				int temp = arr[i];
				arr[i] = arr[index];
				arr[index] = temp;
			}
		}
		
		for(int num : arr) {
			System.out.println(num);
		}
	}
}
