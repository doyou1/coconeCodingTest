package cocone;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.ArrayList;
import java.util.StringTokenizer;

public class Graph4 {
	
	public static void main(String[] args) throws NumberFormatException, IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		
		int dN = Integer.valueOf(br.readLine());
		int lN = Integer.valueOf(br.readLine());
		
		
		ArrayList<Integer>[] listArr = (ArrayList<Integer>[]) new ArrayList[lN+1];
		boolean[] checks = new boolean[lN+1];
		checks[1] = true;
		
		for(int i=0;i<lN+1;i++) {
			listArr[i] = new ArrayList<Integer>();
		}
		
		StringTokenizer st;
		for(int i=0; i<lN; i++) {
			st = new StringTokenizer(br.readLine(), " ");
			
			int x = Integer.valueOf(st.nextToken());
			int y = Integer.valueOf(st.nextToken());
			
			listArr[x].add(y);
			listArr[y].add(x);
		}
				
		for(int i=1;i<lN;i++) {
			System.out.print(i + "의 동기는 : ");
			for(int fs : listArr[i]) {
				
				System.out.print(fs + ", ");					
				
				for(int ffs : listArr[fs]) {
					if(checks[ffs] == false || ffs != fs) {
						System.out.print(ffs + ", ");
						checks[ffs] = true;
					}

				}
			}
			System.out.println();
		}
	}
}
