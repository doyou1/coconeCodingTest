package cocone;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.ArrayList;
import java.util.Arrays;

public class GreedTest6 {


	public static void main(String[] args) throws NumberFormatException, IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		
		String strN = br.readLine();
		int result = -1;
		
		if(Integer.valueOf(strN) % 30 == 0) {
			System.out.println(strN);
			
		}else {
			String[] arr = strN.split("");
			Arrays.sort(arr);
			
			for(int x = arr.length-1; x >=0; x--) {
				
				ArrayList<Long> nums = new ArrayList<>();
				int max = (int) Math.pow(10,arr.length+1);
				for(long i=max-1; i>0;i--) {
					nums.add(i);
				}
				
				for(long num : nums) {
					
					if(num % 30 == 0 && String.valueOf(num).contains(arr[x])
							&& String.valueOf(num).length() == arr.length) {
						
						String[] numA = String.valueOf(num).split("");
						Arrays.sort(numA);
						int cnt = 0;
						
						for(int i = 0; i < numA.length; i++) {
							if(arr[i].equals(numA[i])) {
								cnt++;
							}
						}
						if(cnt == numA.length) {
							if(result < num) {
								result = num;
							}
							break;
						}
					}
				}
			}
		}
		
		System.out.println(result);
	}
}