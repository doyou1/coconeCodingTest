package vo;

public class GugudanTest {

	
	public static void main(String[] args) {
		
		
		int[][] a = new int[5][5];
		
		
		int[][] b = new int[5][5];
		
		int cnt = 0;
		for(int i=0;i<a.length; i++) {
			for(int j=0;j<a[i].length; j++) {
				a[i][j] = ++cnt;

				//a[0][0] = 1
				//a[0][1] = 2
				//a[0][2] = 3
				//a[0][3] = 4
				//a[0][4] = 5
				//
			}
		}

		//2차원배열
		//
		
		for(int i=0;i<b.length; i++) {
			for(int j=0;j<b[i].length; j++) {
				b[i][j] = a[4-j][i];
			}
		}
		
		
		
	}
	
	
}
