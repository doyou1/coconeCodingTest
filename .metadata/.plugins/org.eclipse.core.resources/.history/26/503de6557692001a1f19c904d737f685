	public int dfs(
			int x
			, int[][] vertex
			, int length
			, int cnt) {
		
		if(x != 1 && length == 0) {
			System.out.println("³¡ : " + x);
			return 1;
		}
		length = 0;
		System.out.println("x : " + x);
		for(int[] v : vertex) {
			if(v[0] == x) {
				dfs(v[1],vertex, length++, cnt++);		
			}
		}
		
		return 1;
	}
