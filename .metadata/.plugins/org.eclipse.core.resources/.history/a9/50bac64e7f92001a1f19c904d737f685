package cocone;

public class DPSolution1 {

	int cCount = 0;
	
	public static void main(String[] args) {
		DPSolution1 s1 = new DPSolution1();
		
		int N = 5;
		int number = 12;
		
		s1.solution(N, number);
	}
	
	public int solution(int N, int number) {
		int answer = 0;
		
		int count = 0;
		rs(N, number, N, count);
		
		System.out.println(cCount);
		
		return answer;
	}
	
	public void rs(int N, int number, int temp, int count) {
		if(temp == number) {
			if(cCount == 0 || count < cCount) {
				cCount = count;
			}
			return;
		}else if(count > 8) {
			return;
		}
		
		try {
			rs(N, number,Integer.valueOf(String.valueOf(temp) + String.valueOf(N)),count++);			
		}catch(Exception e) {
			
		}
		
		rs(N, number,temp + N,count++);
		rs(N, number,temp / N,count++);
		
	}
}
